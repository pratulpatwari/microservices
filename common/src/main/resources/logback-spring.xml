<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{dd-MM-yyyy HH:mm:ss.SSS}|%mdc|%magenta([%thread])|%highlight(%-5level)|%logger{36}.%M|%msg%n</pattern>
		</encoder>
	</appender>
	<root level="error">
		<appender-ref ref="STDOUT" />
	</root>
	<logger name="dev.pratul" level="debug"/>
	
</configuration>